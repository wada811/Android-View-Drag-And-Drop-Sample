<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wada811.android_view_drag_and_drop_sample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleDragAndDropActivity"
            android:label="@string/title_activity_simple_drag_and_drop" >
        </activity>
        <activity
            android:name=".ListDragAndDropActivity"
            android:label="@string/title_activity_list_drag_and_drop" >
        </activity>
        <activity
            android:name=".RecyclerDragAndDropActivity"
            android:label="@string/title_activity_recycler_drag_and_drop" >
        </activity>
        <activity
            android:name=".ClipChildrenActivity"
            android:label="@string/title_activity_clip_children" >
        </activity>
    </application>

</manifest>
